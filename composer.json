{
    "name": "kleiven/laravel-ui",
    "description": "Front end scaffolding for Laravel applications.",
    "homepage":"https://github.com/kleiven/laravel-ui",
    "keywords": ["front end scaffolding", "single page application", "spa", "vue", "laravel", "ui"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eivind Kleiven",
            "email": "eivind.kleiven@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/filesystem": "^7.0",
        "illuminate/support": "^7.5",
        "laravel/ui": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kleiven\\Laravel\\Ui\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Kleiven\\Laravel\\Ui\\LaravelUiServiceProvider"
            ]
        }
    }
}
